{
  "theme": {
    "name": "Docker",
    "style": "black",
    "version": "0.1"
  },

  "httpProxy": {
    "url": null,
    "useProxy": true,
    "alwaysUseProxy": false
  },

  "logo": "images/logo.png",
  "logoLink": "http://www.google.com",
  "title": "GeoApp Framework",

  "bodhiOnTouch": {
    "panel": {
      "uri": "stem/TouchableBodhiPanel"
    },
    "bodhis": [{
      "uri": "themes/Docker/widgets/DockerController/Bodhi",
      "position": {
        "left": 0,
        "top": 0,
        "bottom": 0,
        "width": 70
      }
    }, {
      "uri": "bodhis/HomeButton/Bodhi",
      "position": {
        "left": 8,
        "top": 70,
        "zIndex": 0,
        "relativeTo": "map"
      }
    }, {
      "uri": "bodhis/MyLocation/Bodhi",
      "position": {
        "left": 9,
        "top": 107,
        "zIndex": 0,
        "relativeTo": "map"
      }
    }]
  },
  "map": {
    "domId": "mapPlaceholder",
    "uri": "stem/MapModule",
    "position": {
      "left": 70,
      "right": 0,
      "top": 0,
      "bottom": 0,
      "width": "100%",
      "height": "100%"
    }
  },
  "bodhiInController": {
    "panel": {
      "uri": "themes/Docker/panels/Panel",
      "position": {
        "top": 0,
        "left": 70,
        "bottom": 0
      }
    },
    "bodhis": [{
      "uri": "bodhis/HomeButton/Bodhi",
      "position": {
        "left": 8,
        "top": 70,
        "zIndex": 0,
        "relativeTo": "map"
      }
    }, {
      "uri": "bodhis/MyLocation/Bodhi",
      "position": {
        "left": 9,
        "top": 107,
        "zIndex": 0,
        "relativeTo": "map"
      }
    }]
  },
  "version": 0.1
}